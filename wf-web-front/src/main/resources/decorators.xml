<?xml version="1.0" encoding="ISO-8859-1"?>
<decorators defaultdir="/WEB-INF/decorators">
	<!-- Any urls that are excluded will never be decorated by Sitemesh -->
	<excludes>
		<pattern>*ajax*</pattern>
		<pattern>*adhoc/crosstab*</pattern>
		<pattern>*adhoc/table*</pattern>
		<pattern>*adhoc/chart*</pattern>
		<pattern>*adhoc/dashboardDesignerState*</pattern>
		<pattern>*adhoc/dashboardDesignerControls*</pattern>
		<pattern>*viewAsDashboardFrame=true*</pattern>
		<pattern>*login_welcome*</pattern>
		<pattern>*login_mt*</pattern>
		<pattern>*login_oem*</pattern>
		<pattern>*RepositoryExplorerAjaxTable*</pattern>
		<pattern>*ajaxresponse*</pattern>
		<pattern>*print=true*</pattern>
		<pattern>*decorate=no*</pattern>
		<pattern>*tableSubPage*</pattern>
		<pattern>*tableSamePage*</pattern>
	</excludes>

	<decorator name="main" page="main.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>
